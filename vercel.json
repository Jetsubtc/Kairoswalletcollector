{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/test.js": {
      "runtime": "nodejs18.x"
    },
    "api/wallets.js": {
      "runtime": "nodejs18.x"
    },
    "api/admin/wallets.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/wallets",
      "dest": "/api/wallets.js"
    },
    {
      "src": "/api/admin/wallets",
      "dest": "/api/admin/wallets.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}